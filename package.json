  "homepage": "https://myobu-guardians.github.io/hikari-dev/",
  "scripts": {
    "git-info": "echo export default \"{\\\"logMessage\\\": \\\"$(git log -1 --oneline)\\\",\\\"hash\\\":\\\"$(git rev-parse HEAD)\\\"}\"  > src/git_commit.js",
    "start": "react-scripts start",
    "build": "yarn git-info && react-scripts build && yarn build:service-worker",
    "build:local": "yarn git-info && node before-build.js http://127.0.0.1:8080 && react-scripts build && node after-build.js && yarn build:service-worker",
    "build:service-worker": "gulp service-worker",
    "build:splash-screen": "cd public && npx pwa-asset-generator logo.png ./assets -i ./index.html -m manifest.json -p 0 -o false --favicon",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/toastr": "^2.1.39",
    "autoprefixer": "^10.4.7",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
