    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-i18next": "^8.1.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "daisyui": "^2.15.4",
    "ethers": "^5.6.9",
    "i18next": "^21.8.11",
    "is_js": "^0.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.3",
    "react-ios-pwa-prompt": "^1.8.4",
    "react-screen-orientation": "^0.0.4",
    "react-scripts": "^5.0.1",
    "toastr": "^2.1.4",
    "typescript": "^4.7.4",
    "unstated-next": "^1.1.0",
    "web-vitals": "^2.1.4",
    "web3": "^1.7.4"
  },
